{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "globals": {
        
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-expressions":"off",
        "no-unused-vars":"off",
        "no-useless-return": "off",
        "no-restricted-globals":"off",
        "no-unused-labels": "off",
        "no-else-return":"off",
        "no-eval":"off",
        "default-case": "off",
        "radix":"off",
        "prettier/prettier": "error",
        "class-methods-use-this":"off",
        "prefer-const":"off"
    }
}
